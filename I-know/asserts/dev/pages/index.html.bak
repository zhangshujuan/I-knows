		$(".show-all").click(function(){
			if($(".panel-appraise-body").hasClass("panel-hidden")){ 
				$(".panel-appraise-body").removeClass("no-border");
				$(".panel-hidden").slideDown("3000");
				$(".panel-appraise-body:last").addClass("no-border");
			}			
		});
		var consultNum = $(".consult-num").text();
		$(".consult-info-bg i").click(function(){
			if($(this).hasClass("not-collect")){
				$(".consult-info-bg i").removeClass("not-collect").addClass("has-collect");
				consultNum = parseInt(consultNum)+1;
				$(".consult-num").text(consultNum);
			} else {
				$(".consult-info-bg i").addClass("not-collect").removeClass("has-collect");
				consultNum = parseInt(consultNum)-1;
				$(".consult-num").text(consultNum);
			}
		});
	});
